[logic]
active = ph_door@closed

[ph_door@closed]
closed = true
locked = false
on_use = {=actor_community(bandit)} ph_door@opened
snd_open_start = wood_large_open
snd_close_start = wood_large_close_start
snd_close_stop = wood_large_close_stop

[ph_door@opened]
closed = false
locked = false
on_use = {=actor_community(bandit)} ph_door@closed
snd_open_start = wood_large_open
snd_close_start = wood_large_close_start
snd_close_stop = wood_large_close_stop